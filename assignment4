			case 3:
				System.out.println("You have 31 days");
				break;
			case 4:
				System.out.println("You have 30 days");
				break;
			case 5:
				System.out.println("You have 31 days");
				break;
			case 6:
				System.out.println("You have 30 days");
				break;
			case 7:
				System.out.println("You have 31 days");
				break;
			case 8:
				System.out.println("You have 31 days");
				break;
			case 9:
				System.out.println("You have 30 days");
				break;
